	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
		<headerTitle>
			<ObjectPageHeader objectTitle="Social Application" objectSubtitle="SOA Number"></ObjectPageHeader>
		</headerTitle>
		<headerContent>
