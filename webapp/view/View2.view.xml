<core:View xmlns:core="sap.ui.core"
		   xmlns="sap.uxap"
		   xmlns:layout="sap.ui.layout"
		   xmlns:m="sap.m"
		   xmlns:SSPDetails="com.acc.trainingTestDemo.SharedBlocks.SSPDetails"
		   height="100%"
		  >
 
	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
		<headerTitle>
			<ObjectPageHeader objectTitle="SOA ID"
							  objectSubtitle="SOA Number" >
	    </ObjectPageHeader>
		</headerTitle>
 
		<headerContent>
			<m:ObjectAttribute title="" text="SOA Details"/>
		</headerContent>
		
	 	<sections>
		<ObjectPageSection id="SSPDetails" title="SSP Details">
				<subSections>
					<ObjectPageSubSection id="SSPDetailsSS" title=" ">
						<blocks>
							<SSPDetails:SSPBlock id="SSPBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			
			<ObjectPageSection id="BenefitDetails" title="Benefit Details">
				<subSections>
					<ObjectPageSubSection id="BenefitDetailsSS" title=" ">
						<blocks>
							<SSPDetails:SSPBlock id="BenefitBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			
			<ObjectPageSection id="PartnerDetails" title="Partner Details">
				<subSections>
					<ObjectPageSubSection id="PartnerDetailsSS" title=" ">
						<blocks>
							<SSPDetails:SSPBlock id="PartnerBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
 		</sections>
 	</ObjectPageLayout>
</core:View>