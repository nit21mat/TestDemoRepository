<core:View xmlns:core="sap.ui.core" xmlns="sap.uxap" xmlns:layout="sap.ui.layout" xmlns:m="sap.m"
	xmlns:SSPDetails="com.acc.trainingTestDemo.SharedBlocks.SSPDetails"
	xmlns:BenefitDetails="com.acc.trainingTestDemo.SharedBlocks.BenefitDetails"
	xmlns:PartiesInvolved="com.acc.trainingTestDemo.SharedBlocks.PartiesInvolved" height="100%"
	controllerName="com.acc.trainingTestDemo.controller.View2">
	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
		<headerTitle>
			<ObjectPageHeader objectTitle="Social Application"></ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<m:ObjectAttribute title="Soa Id" text="{soaModel>/SOAHeaderDetails/Soaid}"/>
			<m:ObjectAttribute title="SSP ID" text="{soaModel>/SOAHeaderDetails/Sspid}"/>
			<m:ObjectAttribute title="SOA Status" text="{soaModel>/SOAHeaderDetails/Soastatustext}"/>
			<m:ObjectAttribute title="Case ID" text="{soaModel>/SOAHeaderDetails/Caselink}"/>
		</headerContent>
		<sections>
			<ObjectPageSection id="SSPDetails" title="SSP Details">
				<subSections>
					<ObjectPageSubSection id="SSPDetailsSS" title=" ">
						<blocks>
							<SSPDetails:SSPBlock id="SSPBlock">
								<SSPDetails:mappings>
									<ModelMapping externalModelName="soaModel" externalPath="{sViewPath}/SSPHeaderDetailsSet" internalModelName="SSP"/>
								</SSPDetails:mappings>
							</SSPDetails:SSPBlock>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="BenefitDetails" title="Benefit Details">
				<subSections>
					<ObjectPageSubSection id="BenefitDetailsSS" title=" ">
						<blocks>
							<BenefitDetails:BenefitBlock id="BenefitBlock">
								<BenefitDetails:mappings>
									<ModelMapping externalModelName="soaModel" externalPath="{sViewPath}/SOABenefitsSet" internalModelName="Benefits"/>
								</BenefitDetails:mappings>
							</BenefitDetails:BenefitBlock>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="PartnerDetails" title="Partner Details">
				<subSections>
					<ObjectPageSubSection id="PartnerDetailsSS" title="">
						<blocks>
							<PartiesInvolved:PartiesBlock id="PartnerBlock">
								<PartiesInvolved:mappings>
									<ModelMapping externalModelName="soaModel" externalPath="{sViewPath}/SOAPartnerFunctionsSet" internalModelName="PartnerFunction"/>
								</PartiesInvolved:mappings>
							</PartiesInvolved:PartiesBlock>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</core:View>