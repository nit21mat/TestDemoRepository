			<m:ObjectAttribute title="SOA Status" text="{ path: '/SOAHeaderDetailsSet', sorter: { path: 'Soastatus' } }"/>
			<m:ObjectAttribute title="Case ID" text="{ path: '/SOAHeaderDetailsSet', sorter: { path: 'Caselink' } }"/>
-->
			<m:ObjectAttribute title="Soa Id" text="{Soaid}"/>
			<m:ObjectAttribute title="SSP ID" text="{Sspid}"/>
