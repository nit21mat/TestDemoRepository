<Table id="PartiesTable" inset="false" growing="true" growingThreshold="10" growingScrollToLoad="false"
			items="{ path: '/SOASearchResult__PartnerFunctions', sorter: { path: 'SoaIdRes' } }">
			<columns>
				<Column width="12em">
					<Text text="Partner No"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true">
					<Text text="Partner Name"/>
				</Column>
				<Column hAlign="Center">
					<Text text="Partner Function"/>
				</Column>
				<Column hAlign="Center">
					<Text text="SOA Ref"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem type="Navigation" press="onSelectionChange">
					<cells>
						<ObjectIdentifier text="{Partnerid}"/>
						<Text text="{Partnername}"/>
						<Text text="{Partnerfunction}"/>
						<Text text="{Soaref}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	<!--</forms:SimpleForm>-->
</mvc:View>