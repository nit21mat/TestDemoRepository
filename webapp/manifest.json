{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.acc.trainingTestDemo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"ZSOA_SEARCH_DETAILS_SRV": {
				"uri": "/sap/opu/odata/sap/ZSOA_SEARCH_DETAILS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "model/metadata.xml"
				}
			},
			"status_alias": {
				"uri": "./model/Status.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.acc.trainingTestDemo.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.acc.trainingTestDemo.i18n.i18n"
				}
			},
			"status": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "status_alias"
			},
			"soaModel": {
				"dataSource": "ZSOA_SEARCH_DETAILS_SRV",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.acc.trainingTestDemo.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "View1",
					"target": "View1"
				},
				{
					"pattern": "View2/{soaId}",
					"name": "View2",
					"target": "View2"
				}
			],
			"targets": {
				"View1": {
					"viewName": "View1"
				},
				"View2": {
					"viewName": "View2"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/ztestdemo/webapp",
		"_version": "1.1.0"
	}
}