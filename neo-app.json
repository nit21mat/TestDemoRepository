{
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources",
        "version": "1.44.10"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources",
        "version": "1.44.10"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/sap/opu/odata",
      "target": {
        "type": "destination",
        "name": "AccentureSSS",
        "entryPath": "/sap/opu/odata"
      },
      "description": "AccentureSSS"
    },
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/sap/bc/ui5_ui5",
      "target": {
        "type": "destination",
        "name": "AccentureSSS",
        "entryPath": "/sap/bc/ui5_ui5"
      },
      "description": "Accenture"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "**.html"
    },
    {
      "directive": "private",
      "maxAge": 0,
      "path": "sap-ui-cachebuster-info.json"
    },
    {
      "directive": "public",
      "maxAge": 31536000
    }
  ]
}